<%= render 'books/list',{user: @user,book: @book} %>

<h2>Users</h2>
  <table>
    <thead>
      <tr>
        <th>Image</th>
        <th>name</th>
        <th></th>
      </tr>
    </thead>
    
    <tbody>
      <% @users.each do |user| %>
      <tr>
       <td><%= image_tag user.get_profile_image(100,100) %></td>
       <td><%= user.name %></td>
      </tr>
      <% end %>
    </tbody>
    
  </table>